<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="20dp"
    tools:context=".RetrieveActivity">

    <TextView
        android:id="@+id/outPutID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:hint="@string/output_text"
        android:textSize="20sp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:textStyle="bold|italic"
        android:padding="10dp"
        />

    <Button
        android:id="@+id/retrievePublicID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/outPutID"
        android:layout_marginTop="60dp"
        android:text="public"
        android:onClick="PublicRetrieve"
        android:layout_alignParentStart="true"
        />

    <Button
        android:id="@+id/retrievePrivateID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/outPutID"
        android:layout_centerInParent="true"
        android:layout_marginTop="60dp"
        android:text="private"
        android:onClick="PrivateRetrieve"
        />


    <Button
        android:id="@+id/backID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/outPutID"
        android:layout_marginTop="60dp"
        android:text="Back"
        android:onClick="Back"
        android:layout_alignParentEnd="true"
        />


</RelativeLayout>